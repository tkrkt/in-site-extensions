{
  "name": "__MSG_appName__",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "downloads"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/icon-128.png"
    },
    "default_title": "In-Site Bookmark",
    "default_popup": "popup.html"
  }
}
