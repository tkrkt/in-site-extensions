{
  "name": "__MSG_appName__",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["dist/background.bundle.js"]
  },
  "permissions": ["tabs", "storage", "downloads"],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "32": "icon-32.png",
      "64": "icon-64.png",
      "128": "icon-128.png"
    },
    "default_title": "In-Site Bookmark",
    "default_popup": "popup.html"
  }
}